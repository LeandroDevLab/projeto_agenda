<!-- Para pegar esses errors que ainda nÃ£o existem eu crio um locals 
 com middleware e carrego os errors -->
<% if(errors.length > 0) { %>
<div class="row">
  <div class="col my-3">
    <div class="alert alert-danger">
        <% errors.forEach(erro =>  { %>
            <%= erro %> <br>
        <% }); %>
    </div>
  </div>
</div>
<% } %>

<% if(success.length > 0) { %>
<div class="row">
  <div class="col my-3">
    <div class="alert alert-success">
        <% success.forEach(success =>  { %>
            <%= success %> <br>
        <% }); %>
    </div>
  </div>
</div>
<% } %>
